# Git Commit Manager 설정

# 기본 LLM 프로바이더 설정
# 지원 프로바이더: ollama, openrouter, gemini
DEFAULT_PROVIDER=ollama

# 기본 모델 설정
# Ollama 예시: gemma3:1b, codellama, qwen2.5-coder:1.5b
# OpenRouter 예시: openai/gpt-3.5-turbo, anthropic/claude-3-haiku
# Gemini 예시: gemini-pro, gemini-1.5-flash
DEFAULT_MODEL=gemma3:1b

# LLM API 키
OPENROUTER_API_KEY=your-openrouter-api-key-here
GEMINI_API_KEY=your-gemini-api-key-here

# 디바운스 설정 (초 단위) - 파일 변경 후 분석까지의 대기 시간
DEBOUNCE_DELAY=3.0

# 커밋 메시지 언어 설정
# 지원 언어: korean, english, japanese, chinese, spanish, french, german
COMMIT_MESSAGE_LANGUAGE=korean

# 자동 코드 리뷰 활성화 여부 (true/false)
# false로 설정하면 커밋 메시지만 생성하고 코드 리뷰는 수행하지 않음
AUTO_CODE_REVIEW=true